@model SMCPagerModel<ConsultaConsolidadaProcessoListaViewModel>

@(Smc.Grid("gridProcesso")
        .AllowExportToExcel(true)       
        .Button(Smc.Button(SMCMvcConsts.MODAL_BEHAVIOR_OPEN)
            .Attribute(SMCMvcConsts.MODAL_ID, "modalLegendaPosicaoConsolidada")
            .Text(Smc.GetUIResource("Botao_LegendaPosicao"))
                .CssClass("smc-btn-ico-ajuda")) 
        .AutoGenerateColumns()                 
        .ButtonColumn(SMCButtonSetType.Secondary, 0, "", "", "smc-grid-coluna-botoes",
            item => Smc.Button("ConsultaInscricoesProcesso", "ConsultarInscricaoProcesso", new { @seqProcesso = SMCDESCrypto.EncryptNumberForURL((item as ConsultaConsolidadaListaViewModel).Seq) }, "AcompanhamentoProcesso")
                       .Type(SMCButtonType.Link)
                       .SecurityToken(UC_INS_003_01_03.CONSULTA_INSCRICOES_PROCESSO),
                       
            item => Smc.Button("ConsultaPosiçãoConsolidadaGrupoOFertasOFerta", "ConsultarProcessoPorGrupoOferta", new { @seqProcesso = SMCDESCrypto.EncryptNumberForURL((item as ConsultaConsolidadaListaViewModel).Seq) }, "AcompanhamentoProcesso")
                       .Type(SMCButtonType.Link)
                       .SecurityToken(UC_INS_003_01_02.CONSULTA_POSICAO_CONSOLIDADA_GRUPO_OFERTA_OFERTA),
                       
            item => Smc.Button("AnaliseInscricaoLote", "AnalisarInscricaoLote", new { @seqProcesso = SMCDESCrypto.EncryptNumberForURL((item as ConsultaConsolidadaListaViewModel).Seq) }, "AcompanhamentoProcesso")
                       .Type(SMCButtonType.Link)
                       .SecurityToken(UC_INS_003_01_04.ANALISE_INSCRICAO_LOTE)
    )
)




