@{

    var buttons = Smc.ButtonSet();

    if (!string.IsNullOrEmpty(Model.BackURL))
    {
        buttons.AddButton(Smc.Button("Voltar")
        .Type(SMCButtonType.Link)
        .CssClass("smc-btn-voltar-icotext")
        .Url(Model.BackURL));

    }
    else
    {
        buttons.AddButton(Smc.Button(SMCButtonBehavior.Voltar).DisplayMode(SMCButtonDisplayMode.TextAndIcon));
    }

}

@(buttons)