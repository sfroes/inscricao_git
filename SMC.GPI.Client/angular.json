{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "administrativo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/smc.gpi.administrativo",
      "sourceRoot": "projects/smc.gpi.administrativo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/administrativo",
            "index": "projects/smc.gpi.administrativo/src/index.html",
            "browser": "projects/smc.gpi.administrativo/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/smc.gpi.administrativo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "baseHref": "/Dev/GPI.Administrativo/App/",
            "assets": [
              "projects/smc.gpi.administrativo/src/favicon.ico",
              "projects/smc.gpi.administrativo/src/assets"
            ],
            "styles": [
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "projects/smc.gpi.administrativo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "fonts": false
              },
              "fileReplacements": [
                {
                  "replace": "projects/smc.gpi.administrativo/src/environments/environment.ts",
                  "with": "projects/smc.gpi.administrativo/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "administrativo:build:production"
            },
            "development": {
              "buildTarget": "administrativo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "administrativo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/smc.gpi.administrativo/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/smc.gpi.administrativo/src/favicon.ico",
              "projects/smc.gpi.administrativo/src/assets"
            ],
            "styles": [
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "projects/smc.gpi.administrativo/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
