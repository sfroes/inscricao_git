      <endpoint name="SMC.Framework.Security.ISMCAuthorizationService" address="net.pipe://localhost/Dev/Seguranca/v4/AutorizacaoService.svc" binding="netNamedPipeBinding" bindingConfiguration="SMCNetPipeLarge" contract="SMC.Framework.Security.ISMCAuthorizationService" />
